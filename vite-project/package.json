{
    "name": "notes-web-app",
    "version": "1.0.0",
    "main": "./src/index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
      "build": "NODE_ENV=production env-cmd -f .env.${ENV:=local} --fallback --silent webpack --progress",
      "commit": "git-cz",
      "start": "env-cmd -f .env.${ENV:=local} --no-override --fallback --silent webpack serve",
      "prepare": "husky install",
      "test:unit": "env-cmd -f .env.test jest $ADDITIONAL_JEST_ARGUMENTS",
      "test:e2e": "env-cmd -f .env.test start-server-and-test 'webpack serve' 8080 \"cypress run $ADDITIONAL_CYPRESS_ARGUMENTS\"",
      "test:open": "env-cmd -f .env.test cypress open",
      "test": "yarn test:unit && yarn test:e2e",
      "lint": "eslint '*/**/*.{ts,tsx}'",
      "lint:fix": "eslint --fix '*/**/*.{ts,tsx}'",
      "package": "mkdir -p package && tar -C build -zcvf package/notes-web-app-${ENV}.tgz .",
      "storybook": "start-storybook -p 6006",
      "storybook:build": "build-storybook",
      "sourcemap:upload": "env-cmd -f .env.${ENV:=local} --fallback --silent sh scripts/uploadSourcemap.sh",
      "report-bundle-size": "node scripts/reportBundleSize.js"
    },
