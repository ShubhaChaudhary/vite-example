    "babelMacros": {
      "twin": {
        "preset": "styled-components",
        "styled": {
          "import": "default",
          "from": "styled-components"
        },
        "css": {
          "import": "css",
          "from": "styled-components"
        }
      }
    },
    "nyc": {
      "report-dir": "cypress-coverage"
    },
    "volta": {
      "node": "18.14.2",
      "yarn": "1.22.19"
    },
    "jest-junit": {
      "addFileAttribute": "true",
      "outputName": "junit-jest.xml"
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
        "jiraOptional": true,
        "jiraPrefix": "NP"
      }
    },
    "lint-staged": {
      "**/*.{js,jsx,ts,tsx}": [
        "yarn prettier --write",
        "yarn eslint --fix"
      ],
      "**/*.{md,json}": "yarn prettier --write"
    },
